  "type": "jig.list",
  "datasources": {
    "swipeable-list": {
      "options": {
        "data": [
          {
            "icon": "home",
            "id": 1,
            "subtitle": "Subtitle",
            "title": "Title"
          },
          {
            "icon": "cog-double",
            "id": 2,
