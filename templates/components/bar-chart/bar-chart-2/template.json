{
  "type": "component.bar-chart",
  "options": {
    "chart": {
      "height": 320,
      "isAnimated": false
    },
    "legend": {
      "isHidden": false
    },
    "plotOptions": {
      "series": {
        "pointWidth": 10
      }
    },
    "series": [
      {
        "color": "color1",
        "data": "=@ctx.datasources.y2019",
        "name": "2019"
      },
      {
        "color": "color2",
        "data": "=@ctx.datasources.y2020",
        "name": "2020"
      },
      {
        "color": "color3",
        "data": "=@ctx.datasources.y2021",
        "name": "2021"
      },
      {
        "color": "color4",
        "data": "=@ctx.datasources.y2022",
        "name": "2022"
      }
    ],
    "xAxis": {
      "categories": ["Q1", "Q2", "Q3", "Q4"],
      "tickAmount": 4
    },
    "yAxis": {
      "labels": {
        "format": {
          "currency": "USD",
          "numberStyle": "currency"
        }
      },
      "max": 60000,
      "min": 10000
    }
  }
}
