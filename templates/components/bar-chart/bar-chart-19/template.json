{
  "type": "component.bar-chart",
  "options": {
    "chart": {
      "title": "Custom name of series"
    },
    "legend": {
      "isHidden": false
    },
    "plotOptions": {
      "series": {
        "pointWidth": 16
      }
    },
    "series": [
      {
        "color": "color6",
        "data": "=@ctx.datasources.serie1",
        "name": "My data 1"
      },
      {
        "color": "color7",
        "data": "=@ctx.datasources.serie2",
        "name": "My data 2"
      },
      {
        "color": "color8",
        "data": "=@ctx.datasources.serie3",
        "name": "My data 3"
      }
    ],
    "xAxis": {
      "categories": ["1", "2", "3", "4"]
    },
    "yAxis": {
      "max": 10000,
      "min": 0
    }
  }
}
